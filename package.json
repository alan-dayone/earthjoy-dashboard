{
  "name": "int-nextjs-ts",
  "version": "1.0.0",
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node server.ts",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "lint": "gts check",
    "lint:fix": "gts fix",
    "lint:clean": "gts clean",
    "build:server": "tsc -p .",
    "build:client": "next build",
    "clean": "rm -rf .next build"
  },
  "dependencies": {
    "@zeit/next-sass": "^0.2.1-canary.4",
    "@zeit/next-typescript": "^1.1.1",
    "ag-grid-community": "^21.0.1",
    "ag-grid-react": "^21.0.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.19.1",
    "lodash": "^4.17.11",
    "next": "^8.1.0",
    "next-redux-wrapper": "^3.0.0-alpha.3",
    "next-routes": "^1.4.2",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "reactstrap": "^8.0.0",
    "redux-crud": "^3.3.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/js-cookie": "^2.2.2",
    "@types/next": "^8.0.5",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/http-proxy-middleware": "^0.19.2",
    "gts": "^1.0.0",
    "husky": "^2.3.0",
    "nodemon": "^1.19.1",
    "ts-node": "8.1.0",
    "typescript": "^3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:dayone-teams/int-boilerplates/int-nextjs-ts.git"
  },
  "author": "",
  "license": "ISC",
  "description": ""
}
